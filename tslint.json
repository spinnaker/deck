{
  "extends": ["tslint:latest", "tslint-react", "tslint-eslint-rules", "tslint-config-prettier"],
  "rules": {
    "ban-types": false,
    "class-name": true,
    "comment-format": [true, "check-space"],
    "curly": true,
    "eofline": true,
    "forin": true,
    "import-blacklist": [
      true,
      "amazon",
      "appengine",
      "azure",
      "canary",
      "cloudfoundry",
      "core",
      "dcos",
      "docker",
      "ecs",
      "google",
      "huaweicloud",
      "kubernetes",
      "oracle",
      "titus "
    ],
    "indent": [true, "spaces"],
    "jsx-no-lambda": false,
    "jsx-no-multiline-js": false,
    "label-position": true,
    "max-classes-per-file": false,
    "max-line-length": false,
    "member-access": true,
    "member-ordering": false,
    "no-arg": true,
    "no-bitwise": true,
    "no-console": [true, "log", "warn", "debug", "info", "time", "timeEnd", "trace"],
    "no-construct": true,
    "no-debugger": true,
    "no-duplicate-variable": true,
    "no-empty": false,
    "no-empty-interface": false,
    "no-eval": true,
    "no-implicit-dependencies": false,
    "no-inferrable-types": true,
    "no-invalid-template-strings": false,
    "no-namespace": false,
    "no-object-literal-type-assertion": false,
    "no-reference": false,
    "no-shadowed-variable": true,
    "no-string-literal": false,
    "no-submodule-imports": false,
    "no-switch-case-fall-through": true,
    "no-this-assignment": false,
    "no-trailing-whitespace": true,
    "no-unused-expression": [true, "allow-fast-null-checks"],
    "no-use-before-declare": false,
    "no-var-keyword": true,
    "no-var-requires": false,
    "object-curly-spacing": "always",
    "object-literal-shorthand": false,
    "object-literal-sort-keys": false,
    "one-line": [true, "check-catch", "check-else", "check-open-brace", "check-whitespace"],
    "one-variable-per-declaration": false,
    "only-arrow-functions": false,
    "ordered-imports": false,
    "prefer-conditional-expression": false,
    "prefer-const": [true, { "destructuring": "all" }],
    "quotemark": [true, "single", "avoid-escape", "jsx-double"],
    "radix": true,
    "semicolon": ["always"],
    "triple-equals": [true, "allow-null-check"],
    "typedef": ["call-signature", "property-declaration"],
    "typedef-whitespace": [
      true,
      {
        "call-signature": "nospace",
        "index-signature": "nospace",
        "parameter": "nospace",
        "property-declaration": "nospace",
        "variable-declaration": "nospace"
      }
    ],
    "variable-name": [true, "ban-keywords", "allow-leading-underscore"],
    "whitespace": [true, "check-branch", "check-decl", "check-operator", "check-separator", "check-type"]
  }
}
